{
    // Enable GitDoc
    "GitDoc: Enabled": true,
  
    // Commit delay: 10 minutes (600,000 ms)
    "GitDoc: AutoCommit Delay": 600000,
  
    // Push delay: 10 minutes (600,000 ms)
    "GitDoc: Autopush Delay": 600000,
  
    // Push automatically after commit
    "GitDoc: Autopush": "afterDelay",
  
    // Default commit message
    "GitDoc: Commit Message": "Auto commit from GitDoc",
  
    // Branch to push changes to
    "GitDoc: Branch": "main",
  
    // Optional: commit even if warnings exist
    "GitDoc: Commit Validation Level": "warning",
    "gitdoc.enabled": true
  }
  